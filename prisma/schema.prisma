generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Pratiques {
  id Int @default(autoincrement()) @id
  date String?
  type String
  note Int
  dateTimeStart String
  duration Int
  dateTimeEnd String
  useProducts String?
}

model Spots {
  id Int @default(autoincrement()) @id
  name String
  description String?
  latitude Int
  longitude Int
  authorId Int?
}

model User {
  id    Int     @default(autoincrement()) @id
  email String  @unique
  lastname  String?
  firstname String?
  password  String
  token String?
  tokenDate String?
  powerLevel Int
}
